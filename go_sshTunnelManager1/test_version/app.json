{
  "debug": true,
  "timeout_seconds": 3,
  "reconnect_interval_seconds": 2,
  "log_file": "app.log",
  "ssh_settings": {
    "mac_algorithms": [
      "hmac-sha1",
      "hmac-sha1-96",
      "hmac-sha2-256",
      "hmac-sha2-512",
      "hmac-md5",
      "hmac-md5-96",
      "umac-64@openssh.com",
      "umac-128@openssh.com"
    ],
    "kex_algorithms": [
      "diffie-hellman-group1-sha1",
      "diffie-hellman-group-exchange-sha1",
      "diffie-hellman-group14-sha1",
      "diffie-hellman-group14-sha256",
      "diffie-hellman-group16-sha512",
      "diffie-hellman-group18-sha512",
      "ecdh-sha2-nistp256",
      "ecdh-sha2-nistp384",
      "ecdh-sha2-nistp521",
      "curve25519-sha256",
      "curve25519-sha256@libssh.org",
      "diffie-hellman-group-exchange-sha256"
    ],
    "host_key_algorithms": [
      "ssh-rsa",
      "ssh-dss",
      "ssh-ed25519",
      "ecdsa-sha2-nistp256",
      "ecdsa-sha2-nistp384",
      "ecdsa-sha2-nistp521"
    ],
    "ciphers": [
      "3des-cbc",
      "aes128-cbc",
      "aes192-cbc",
      "aes256-cbc",
      "aes128-ctr",
      "aes192-ctr",
      "aes256-ctr",
      "aes128-gcm@openssh.com",
      "aes256-gcm@openssh.com",
      "chacha20-poly1305@openssh.com"
    ]
  },
  "global_max_reconnects": 10,
  "global_auto_reconnects": 5
}
